@prefix sdmx-subject:    <http://purl.org/linked-data/sdmx/2009/subject#> .

# -- Data Set --------------------------------------------
stats:sumberdidikpramukaSBY2011
	a qb:DataSet ;
	rdfs:label "Sumber_didik_perkecamatan_surabaya_2011" ;
	qb:structure stats:dsdjumlah_sumber ;
	qb:slice	stats:anggota_kecamatan	.

stats:sasarandidikpramukaSBY2011
	a qb:DataSet ;
	rdfs:label "Sasaran_didik_perkecamatan_surabaya_2011" ;
	qb:structure stats:dsdjumlah_sasaran .

# -- Data structure definition ----------------------------
stats:dsdjumlah_sumber
	qb:component [
qb:measure stats:jumlah_anggota
],[
qb:dimension dimensi:kecamatan
],[
qb:dimension dimensi:tingkatan_sumber_didik
];
a qb:DataStructureDefinition ;
.

stats:dsdjumlah_sasaran
	qb:component [
qb:measure stats:jumlah_anggota
],[
qb:dimension dimensi:kecamatan
],[
qb:dimension dimensi:tingkatan_sasaran_didik
];
a qb:DataStructureDefinition ;
.


# -- Dimensions and measures  -----------------------------
dimensi:tingkatan_sumber_didik a rdf:Property, qb:DimensionProperty;
	rdfs:label "Tingkatan_Sumber_Didik" ;
	rdfs:range skos:Concept ;
	qb:codeList konsep:tingkatan_sumber_didik ;
	.
dimensi:tingkatan_sasaran_didik a rdf:Property, qb:DimensionProperty;
	rdfs:label "Tingkatan_Sasaran_Didik" ;
	rdfs:range skos:Concept ;
	qb:codeList konsep:tingkatan_sasaran_didik ;
	.
dimensi:kecamatan a rdf:Property, qb:DimensionProperty;
	rdfs:label "Kecamatan" ;
	rdfs:range skos:Concept ;
	qb:codeList konsep:kecamatan_surabaya ;
	.
stats:jumlah_anggota a rdf:Property, qb:MeasureProperty ;
	rdfs: label "jumlah_anggota" ;
	rdfs:range xsd:integer ;
	.

konsep:tingkatan_sumber_didik a skos:ConceptScheme, sdmx:CodeList ;
rdfs:label "Tingkatan_Sumber_Didik" ;
skos:hasTopConcept konsep:majelis_pembimbing, konsep:pelatih, konsep:pembina;
skos:notation "tingkatan_sumber" 
.

konsep:tingkatan_sasaran_didik a skos:ConceptScheme, sdmx:CodeList ;
rdfs:label "Tingkatan_Sasaran_Didik" ;
skos:hasTopConcept konsep:penegak, konsep:penggalang, konsep:siaga;
skos:notation "tingkatan_sasaran" 
.

konsep:kecamatan_surabaya a skos:ConceptScheme, sdmx:CodeList ;
rdfs:label "Kecamatan_di_Surabaya" ;
skos:hasTopConcept kecamatan:Tegalsari, kecamatan:Genteng, kecamatan:Bubutan, kecamatan:Simokerto, kecamatan:Pabean_Cantikan, kecamatan:Semampir, kecamatan:Krembangan, kecamatan:Kenjeran, kecamatan:Bulak, kecamatan:Tambaksari, kecamatan:Gubeng, kecamatan:Rungkut, kecamatan:Tenggilis_Mejoyo, kecamatan:Gunung_Anyar, kecamatan:Sukolilo, kecamatan:Mulyorejo, kecamatan:Sawahan, kecamatan:Wonokromo, kecamatan:Karangpilang, kecamatan:Dukuh_Pakis, kecamatan:Wiyung, kecamatan:Wonocolo, kecamatan:Gayungan, kecamatan:Jambangan, kecamatan:Tandes, kecamatan:Sukomanunggal, kecamatan:Asemrowo, kecamatan:Benowo, kecamatan:Pakal, kecamatan:Lakarsantri, kecamatan:Sambikerep ;
skos:notation "Kecamatan_Surabaya" 
.
